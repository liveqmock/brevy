      <description>ErrorCode生成GOAL#caobin#2013-7-22</description>
      <requiresDirectInvocation>false</requiresDirectInvocation>
      <requiresProject>true</requiresProject>
      <requiresReports>false</requiresReports>
      <aggregator>false</aggregator>
      <requiresOnline>false</requiresOnline>
      <inheritedByDefault>true</inheritedByDefault>
      <implementation>com.brevy.fw.plugin.generator.mojo.generator.ErrorCodeGeneratorMojo</implementation>
      <language>java</language>
      <instantiationStrategy>per-lookup</instantiationStrategy>
      <executionStrategy>once-per-session</executionStrategy>
      <parameters>
        <parameter>
          <name>goalErrorCodeGenerator</name>
          <type>com.brevy.fw.plugin.generator.parameter.GoalErrorCodeGenerator</type>
          <required>true</required>
          <editable>true</editable>
          <description>goal为errorCodeGenerator的参数集</description>
        </parameter>
        <parameter>
          <name>resources</name>
          <type>java.util.List</type>
          <required>true</required>
          <editable>false</editable>
          <description>资源文件目录</description>
        </parameter>
        <parameter>
          <name>sourceDirectory</name>
          <type>java.io.File</type>
          <required>true</required>
          <editable>false</editable>
          <description>源码文件目录</description>
        </parameter>
      </parameters>
      <configuration>
        <resources implementation="java.util.List">${project.build.resources}</resources>
        <sourceDirectory implementation="java.io.File">${project.build.sourceDirectory}</sourceDirectory>
      </configuration>
    </mojo>

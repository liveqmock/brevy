package com.brevy.fw.common.support.exception;

/**
 * @Description 系统运行时异常类
 * @author caobin
 * @date 2013-7-23
 * @version 1.0
 */
public class SystemRuntimeException extends CoreRuntimeException {

	private static final long serialVersionUID = 5056324301963070920L;

	public SystemRuntimeException() {
		super();
	}

	public SystemRuntimeException(MessageCode messageCode, Throwable cause) {
		super(messageCode, cause);
	}

	public SystemRuntimeException(MessageCode messageCode) {
		super(messageCode);
	}

	public SystemRuntimeException(String message, Throwable cause) {
		super(message, cause);
	}

	public SystemRuntimeException(String message) {
		super(message);
	}

	public SystemRuntimeException(Throwable cause) {
		super(cause);
	}
}

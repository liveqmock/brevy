package com.brevy.fw.common.core.security.shiro.model;

import java.io.Serializable;

import org.apache.commons.lang.builder.ReflectionToStringBuilder;
import org.apache.commons.lang.builder.ToStringStyle;

public class ApOperPerm implements Serializable {

	private static final long serialVersionUID = 1L;

	private long id;
	
	private long appId;
	
	private String authorizedOper;

	/**
	 * @return the id
	 */
	public long getId() {
		return id;
	}

	/**
	 * @param id the id to set
	 */
	public void setId(long id) {
		this.id = id;
	}

	/**
	 * @return the appId
	 */
	public long getAppId() {
		return appId;
	}

	/**
	 * @param appId the appId to set
	 */
	public void setAppId(long appId) {
		this.appId = appId;
	}

	/**
	 * @return the authorizedOper
	 */
	public String getAuthorizedOper() {
		return authorizedOper;
	}

	/**
	 * @param authorizedOper the authorizedOper to set
	 */
	public void setAuthorizedOper(String authorizedOper) {
		this.authorizedOper = authorizedOper;
	}
	
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return ReflectionToStringBuilder.toString(this, ToStringStyle.MULTI_LINE_STYLE);
	}
}

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
<!-- 指定到Dao类 -->    
<mapper namespace="com.brevy.batch.core.dao.QueryDao">
	<resultMap id="SeqResultMap" type="com.brevy.batch.core.model.ApSeq">
		<id column="SEQ_NAME" property="seqName"/>
		<result column="SEQ_VALUE" property="seqValue"/>
	</resultMap>
	
	<!-- 查询所有记录 -->
	<select id="queryAll" resultMap="SeqResultMap">
		<![CDATA[
			SELECT * FROM AP_SEQ
		]]>
	</select>
	
	<!-- 查询指定记录 -->
	<select id="queryPaged" resultMap="SeqResultMap">
		<![CDATA[
			SELECT * FROM AP_SEQ
		]]>
	</select>
	
</mapper>